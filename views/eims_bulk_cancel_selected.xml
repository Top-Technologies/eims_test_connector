<record id="view_account_move_list_inherit_eims" model="ir.ui.view">
    <field name="name">account.move.list.inherit.eims</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_list"/>
    <field name="arch" type="xml">
        <xpath expr="//list" position="inside">
            <header>
                <button name="action_bulk_cancel_eims_invoices"
                        type="object"
                        string="Bulk Cancel EIMS"
                        class="btn-danger"
                />
            </header>
        </xpath>
    </field>
</record>
